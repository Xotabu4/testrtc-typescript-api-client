{
  "name": "testrtc-typescript-api-client",
  "version": "1.29.0",
  "description": "Unoficial API client for testrtc.com service",
  "main": "./dist/client.js",
  "types": "./dist/client.d.ts",
  "files": [
    "/dist",
    "/types"
  ],
  "scripts": {
    "prepare": "npm run ts-from-swagger && npm run build",
    "build": "tsc",
    "ts-from-swagger": "openapi-typescript https://apidoc.testrtc.com/swagger.json --output ./types/swagger.d.ts"
  },
  "author": "Oleksandr Khotemskyi <xotabu4@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "form-data": "^4.0.0",
    "got": "^11.8.2",
    "http-req-builder": "^1.3.1"
  },
  "devDependencies": {
    "@types/node": "12.12.2",
    "typescript": "4.2.4",
    "openapi-typescript": "^5.0.0"
  }
}
